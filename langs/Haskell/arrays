let a = [3,2,6,14,7,9,23,22]   -- [3,2,6,14,7,9,23,22]
let b = [1..100]               -- [1,2,3,4,5...99,100]
let c = [0,2..100]             -- [0,2,4,6,8...98,100]
let d = [100,99..1]            -- [100,99,98,97...2,1]
let e = [1..]                  -- [1,2,3,4,5,6,7,8...]


a !! 2                         -- return element on position 2, 0-indexed
